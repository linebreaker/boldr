{
  "name": "boldr-icons",
  "version": "0.1.0-rc.1",
  "description": "icons for boldr",
  "main": "lib/boldr-icons.commonjs.js",
  "module:modern": "lib/boldr-icons.esmodules.js",
  "files": ["lib"],
  "scripts": {
    "build": "npm run build:es2015 && npm run build:es2015modules && npm run build:copy-files",
    "build:es2015": "NODE_ENV=production babel ./src --out-dir ./build",
    "build:es2015modules": "NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.es.js",
    "build:copy-files": "babel-node ./copy-files.js",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "publish": "npm run build && cd build && npm publish --access=public",
    "svg": "svg2react-icon raw/fa src/fa",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["icon", "react"],
  "license": "MIT",
  "author": {
    "name": "Steven Truesdell",
    "email": "steven@strues.io",
    "url": "strues.io"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.3.0 || ^16.0.0-beta.1",
    "react-dom": "^15.3.0 || ^16.0.0-beta.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-boldr": "0.11.10",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "fs-extra": "^4.0.0",
    "rimraf": "^2.6.1",
    "rollr": "^0.1.0",
    "svg2react-icon": "^1.0.22"
  },
  "dependencies": {}
}
